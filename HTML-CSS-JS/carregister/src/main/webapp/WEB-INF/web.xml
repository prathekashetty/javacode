<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Audi</servlet-name>
  	<servlet-class>com.wolken.caar.AudiServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>BMW</servlet-name>
  	<servlet-class>com.wolken.caar.BMWServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>Ferrari</servlet-name>
  	<servlet-class>com.wolken.caar.FerrariServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>Ford</servlet-name>
  	<servlet-class>com.wolken.caar.FordServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>MercedesBenz</servlet-name>
  	<servlet-class>com.wolken.caar.MercedesBenzServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>Jaguar</servlet-name>
  	<servlet-class>com.wolken.caar.JaguarServlet</servlet-class>
  </servlet>
  
   <servlet>
  	<servlet-name>Peugeot</servlet-name>
  	<servlet-class>com.wolken.caar.PeugeotServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>RollsRoyce</servlet-name>
  	<servlet-class>com.wolken.caar.RollsRoyceServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Toyota</servlet-name>
  	<servlet-class>com.wolken.caar.ToyotaServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Volkswagen</servlet-name>
  	<servlet-class>com.wolken.caar.VolkswagenServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>Audi</servlet-name>
  	<url-pattern>/Audi cars</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
  	<servlet-name>MercedesBenz</servlet-name>
  	<url-pattern>/MercedesBenz cars</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
  	<servlet-name>BMW</servlet-name>
  	<url-pattern>/BMW cars</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Ferrari</servlet-name>
  	<url-pattern>/Ferrari cars</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Ford</servlet-name>
  	<url-pattern>/Ford cars</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
  	<servlet-name>Jaguar</servlet-name>
  	<url-pattern>/Jaguar cars</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
  	<servlet-name>Peugeot</servlet-name>
  	<url-pattern>/Peugeot cars</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>RollsRoyce</servlet-name>
  	<url-pattern>/RollsRoyce cars</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
  	<servlet-name>Toyota</servlet-name>
  	<url-pattern>/Toyota cars</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
  	<servlet-name>Volkswagen</servlet-name>
  	<url-pattern>/Volkswagen cars</url-pattern>
  </servlet-mapping>
</web-app>

